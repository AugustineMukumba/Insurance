
@{
    ViewBag.Title = "RiskDetail";
    Layout = "~/Views/Shared/_InsuranceMain.cshtml";
}

<html>
<head>

</head>
<body>
    <div class="top-banner-section">
        <div class="container">
            <div class="car-insurance-content">
                <h2>Get Car Insurance In Less<br>Than 5 Minutes<h2>
            </div>
        </div>
    </div>
    <div class="body-content-section">
        <div class="Process-step-section">
            <div class="container">
                <div class="row bs-wizard">
                    <div class="process-block">
                        <div class="col-xs-2 bs-wizard-step complete">
                            <div class="progress"><div class="progress-bar"></div></div>
                            <a href="#" class="bs-wizard-dot"></a>
                            <div class="text-center bs-wizard-stepnum">Client Details</div>
                        </div>

                        <div class="col-xs-2 bs-wizard-step complete">
                            <!-- complete -->
                            <div class="progress"><div class="progress-bar"></div></div>
                            <a href="#" class="bs-wizard-dot"></a>
                            <div class="text-center bs-wizard-stepnum">Product Details</div>
                        </div>

                        <div class="col-xs-2 bs-wizard-step active">
                            <!-- complete -->
                            <div class="progress"><div class="progress-bar"></div></div>
                            <a href="#" class="bs-wizard-dot"></a>
                            <div class="text-center bs-wizard-stepnum">Risk Details</div>
                        </div>

                        <div class="col-xs-2 bs-wizard-step disabled">
                            <!-- active -->
                            <div class="progress"><div class="progress-bar"></div></div>
                            <a href="#" class="bs-wizard-dot"></a>
                            <div class="text-center bs-wizard-stepnum">Summay Details</div>
                        </div>

                        <div class="col-xs-2 bs-wizard-step disabled">
                            <!-- active -->

                            <div class="progress"><div class="progress-bar"></div></div>
                            <a href="#" class="bs-wizard-dot"></a>
                            <div class="text-center bs-wizard-stepnum">Payment Details</div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <div class="form-section-top">
            <div class="container">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 form-section-col">
                        <div class="form-group-section">
                            <div class="form-title">
                                <h4>Risk Details</h4>
                            </div>
                            <div class="form-group-input">
                                <form role="form" id="contact-form" class="contact-form">
                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Client Name" placeholder="Number of Cars Covered">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="email" class="form-control" name="text" autocomplete="off" id="Contact Number" placeholder="Vehicle Registration Number ">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle make">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-select">
                                            <div class="form-group">
                                                <select class="form-control">
                                                    <option>Vehicle Model</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Cubic Capacity">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle year">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Engine Number">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Chassis Number">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Color">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Usage">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12 form-group-select">
                                            <div class="form-group">
                                                <select class="form-control">
                                                    <option>Vehicle Cover type</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control calender" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Cover Start Date">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control calender" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Cover End Date">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Sum Insured">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Premium">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Stamp Duty">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="ZTSC Levy">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Vehicle Radio Licence Cost">
                                            </div>
                                        </div>
                                        <div class="col-sm-6 col-md-6 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Additional/Optional Covers">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-sm-12 col-md-12 form-group-text">
                                            <div class="form-group">
                                                <input type="text" class="form-control" name="Name" autocomplete="off" id="Name" placeholder="Excess">
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-12 next-btn">
                                            <div class="row">
                                                <div class="col-sm-6 col-md-6">
                                                    <div class="text-left">
                                                        <div class="back-left"><button type="submit" class="btn">Back</button></div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6 col-md-6">
                                                    <div class="text-right">
                                                        <button type="submit" class="btn">Next</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
